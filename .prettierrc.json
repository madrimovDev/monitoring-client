{
	"semi": false,
	"endOfLine": "crlf",
	"trailingComma": "none",
	"singleQuote": true,
	"quoteProps": "consistent",
	"jsxSingleQuote": true,
	"singleAttributePerLine": true,
	"printWidth": 120,
	"useTabs": true,
	"tabWidth": 2,
	"bracketSameLine": true,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"proseWrap": "preserve"
}
